{
  "name": "dotenv-parse-variables",
  "description": "Parse dotenv files for Boolean, Array, and Number variable types, built for CrocodileJS",
  "version": "1.0.1",
  "author": "Nick Baugh <niftylettuce@gmail.com>",
  "ava": {
    "serial": true
  },
  "bugs": {
    "url": "https://github.com/niftylettuce/dotenv-parse-variables/issues",
    "email": "niftylettuce@gmail.com"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "contributors": [
    "Nick Baugh <niftylettuce@gmail.com> (http://niftylettuce.com)"
  ],
  "dependencies": {
    "debug": "^4.1.1",
    "is-string-and-not-blank": "^0.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "ava": "^3.12.1",
    "codecov": "^3.7.2",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.8.1",
    "eslint-config-xo-lass": "^1.0.3",
    "eslint-plugin-node": "^11.1.0",
    "fixpack": "^3.0.6",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "nyc": "^15.1.0",
    "remark-cli": "^8.0.1",
    "remark-preset-github": "^3.0.0",
    "xo": "^0.33.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/niftylettuce/dotenv-parse-variables",
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "array",
    "boolean",
    "check",
    "convert",
    "dot",
    "dotenv",
    "env",
    "number",
    "parse",
    "variables"
  ],
  "license": "MIT",
  "lint-staged": {
    "linters": {
      "*.js": [
        "xo --fix",
        "git add"
      ],
      "*.md": [
        "remark . -qfo",
        "git add"
      ],
      "package.json": [
        "fixpack",
        "git add"
      ]
    }
  },
  "main": "src/index.js",
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": true,
    "trailingComma": "none"
  },
  "remarkConfig": {
    "plugins": [
      "preset-github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/niftylettuce/dotenv-parse-variables"
  },
  "scripts": {
    "ava": "cross-env NODE_ENV=test ava",
    "build": "npm run build:clean && npm run build:lib",
    "build:clean": "rimraf lib",
    "build:lib": "babel src --out-dir lib",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "xo && remark . -qfo && eslint lib",
    "nyc": "cross-env NODE_ENV=test nyc ava",
    "test": "npm run build && npm run lint && npm run ava",
    "test-coverage": "npm run build && npm run lint && npm run nyc"
  },
  "xo": {
    "prettier": true,
    "space": true,
    "extends": [
      "xo-lass"
    ]
  }
}
